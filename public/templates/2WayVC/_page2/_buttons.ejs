<div class="chatting-control">
  <ul class="inline control-ul unstyled">

    <li class="control-li">
      <a href="#"><img src="skins/2WayVC/images/icon1.png"></a>
      <span class="timerCounter">00:00:00</span>
    </li>

    <li class="control-li wrtcAddEmailClick">
      <a href="#"><img src="skins/2WayVC/images/icon2.png"></a>
      <span>Add Call</span>
    </li>

    <li class="control-li">
      <a href="#" class="chat"><img src="skins/2WayVC/images/icon3.png"></a>
      <span>Chat</span>
    </li>

    <li class="control-li wrtcLocalMicBtn">
      <a href="#" class="click-img"><img src="skins/2WayVC/images/icon4.png"></a>
